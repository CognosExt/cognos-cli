{
  "name": "cognos-cli",
  "version": "1.3.0",
  "description": "command line tools for IBM Cognos Analytics",
  "main": "index.js",
  "bin": {
    "cognos": "./src/index.js"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "precommit": "git add README.md ; node_modules/.bin/lint-staged",
    "release": "standard-version"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.js": [
      "node_modules/.bin/prettier --write --single-quote",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/CognosExt/cognos-cli.git"
  },
  "keywords": [
    "Cognos"
  ],
  "author": "Reinier Battenbegr",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/CognosExt/cognos-cli/issues"
  },
  "homepage": "https://github.com/CognosExt/cognos-cli#readme",
  "dependencies": {
    "co": "^4.6.0",
    "co-prompt": "^1.0.0",
    "commander": "^3.0.0",
    "documentation": "^12.1.1",
    "download-git-repo": "^2.0.0",
    "jcognos": "^2.1.0",
    "xmldom": "^0.1.27"
  },
  "devDependencies": {
    "cz-conventional-changelog": "^3.0.2",
    "husky": "^3.0.3",
    "lint-staged": "^9.2.1",
    "prettier": "^1.18.2",
    "standard-version": "^7.0.0"
  }
}
