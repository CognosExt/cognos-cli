{
  "name": "cognos-cli",
  "version": "1.4.3",
  "description": "command line tools for IBM Cognos Analytics",
  "main": "index.js",
  "bin": {
    "cognos": "./src/index.js"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "release": "standard-version"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.js": [
      "node_modules/.bin/prettier --write --single-quote"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/CognosExt/cognos-cli.git"
  },
  "keywords": [
    "Cognos"
  ],
  "author": "Reinier Battenberg",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/CognosExt/cognos-cli/issues"
  },
  "homepage": "https://github.com/CognosExt/cognos-cli#readme",
  "dependencies": {
    "co": "^4.6.0",
    "co-prompt": "^1.0.0",
    "commander": "^5.1.0",
    "documentation": "^13.0.0",
    "download-git-repo": "^3.0.2",
    "jcognos": "^2.2.2",
    "xmldom": "^0.3.0"
  },
  "devDependencies": {
    "cz-conventional-changelog": "^3.2.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.6",
    "prettier": "^2.0.5",
    "standard-version": "^8.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "node_modules/.bin/lint-staged"
    }
  }
}
