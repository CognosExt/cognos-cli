{
  "name": "cognos-cli",
  "version": "1.4.5",
  "description": "command line tools for IBM Cognos Analytics",
  "main": "index.js",
  "bin": {
    "cognos": "./src/index.js"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "release": "standard-version",
    "prepare": "husky install"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.js": [
      "node_modules/.bin/prettier --write --single-quote"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/CognosExt/cognos-cli.git"
  },
  "keywords": [
    "Cognos"
  ],
  "author": "Reinier Battenberg",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/CognosExt/cognos-cli/issues"
  },
  "homepage": "https://github.com/CognosExt/cognos-cli#readme",
  "dependencies": {
    "co": "^4.6.0",
    "co-prompt": "^1.0.0",
    "commander": "^7.2.0",
    "documentation": "^13.2.5",
    "download-git-repo": "^3.0.2",
    "jcognos": "^2.2.2",
    "xmldom": "^0.6.0"
  },
  "devDependencies": {
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.0",
    "standard-version": "^9.3.0"
  }
}
